<mvc:View controllerName="ZINFRA_MRS_PLANNING_BOARD.ZINFRA_MRS_PLANNING_BOARD.controller.ResourceView" xmlns="sap.m"                                                                                                                                           
	xmlns:g="sap.gantt.simple" xmlns:core="sap.ui.core" xmlns:axistime="sap.gantt.axistime" xmlns:config="sap.gantt.config"                                                                                                                                       
	xmlns:mvc="sap.ui.core.mvc" xmlns:table="sap.ui.table" xmlns:html="http://www.w3.org/1999/xhtml">                                                                                                                                                             
	<App>                                                                                                                                                                                                                                                         
		<pages>                                                                                                                                                                                                                                                      
			<Page title="Resource View" showNavButton="true" navButtonPress="onNavBackResourceView">                                                                                                                                                                    
				<content>                                                                                                                                                                                                                                                  
					<g:GanttChartContainer>                                                                                                                                                                                                                                   
						<g:toolbar>                                                                                                                                                                                                                                              
							<g:ContainerToolbar/>                                                                                                                                                                                                                                   
						</g:toolbar>                                                                                                                                                                                                                                             
						<g:GanttChartWithTable id="gantt">                                                                                                                                                                                                                       
							<g:axisTimeStrategy>                                                                                                                                                                                                                                    
								<axistime:ProportionZoomStrategy>                                                                                                                                                                                                                      
									<axistime:totalHorizon>                                                                                                                                                                                                                               
										<config:TimeHorizon startTime="20181029000000" endTime="20181129000000"/>                                                                                                                                                                            
									</axistime:totalHorizon>                                                                                                                                                                                                                              
									<axistime:visibleHorizon>                                                                                                                                                                                                                             
										<config:TimeHorizon startTime="20181029000000" endTime="20181129000000"/>                                                                                                                                                                            
									</axistime:visibleHorizon>                                                                                                                                                                                                                            
								</axistime:ProportionZoomStrategy>                                                                                                                                                                                                                     
							</g:axisTimeStrategy>                                                                                                                                                                                                                                   
							<g:table>                                                                                                                                                                                                                                               
								<table:TreeTable selectionMode="None" visibleRowCountMode="Auto" minAutoRowCount="12"                                                                                                                                                                  
									rows="{ path: '/root', parameters: { arrayNames: ['children'], numberOfExpandedLevels: 1 } }">                                                                                                                                                        
									<table:columns>                                                                                                                                                                                                                                       
										<table:Column label="Resources" template="text" resizable="false"/>                                                                                                                                                                                  
									</table:columns>                                                                                                                                                                                                                                      
									<table:rowSettingsTemplate>                                                                                                                                                                                                                           
										<g:GanttRowSettings rowId="{id}" shapes1="{path : 'shapes', templateShareable: 'true'}">                                                                                                                                                             
											<g:shapes1>                                                                                                                                                                                                                                         
												<g:BaseRectangle time="{startTime}" endTime="{endTime}" fill="@sapUiChartPaletteQualitativeHue1" showTitle="true" title="{title}"/>                                                                                                                
											</g:shapes1>                                                                                                                                                                                                                                        
										</g:GanttRowSettings>                                                                                                                                                                                                                                
									</table:rowSettingsTemplate>                                                                                                                                                                                                                          
								</table:TreeTable>                                                                                                                                                                                                                                     
							</g:table>                                                                                                                                                                                                                                              
						</g:GanttChartWithTable>                                                                                                                                                                                                                                 
					</g:GanttChartContainer>                                                                                                                                                                                                                                  
				</content>                                                                                                                                                                                                                                                 
			</Page>                                                                                                                                                                                                                                                     
		</pages>                                                                                                                                                                                                                                                     
	</App>                                                                                                                                                                                                                                                        
</mvc:View>                                                                                                                                                                                                                                                    